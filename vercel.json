{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.blothera.com"
        }
      ],
      "destination": "https://www.blothera.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "blothera.com"
        }
      ],
      "destination": "https://www.blothera.com/:path*",
      "permanent": true
    }
  ],
  "trailingSlash": false
}
