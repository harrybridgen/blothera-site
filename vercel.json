{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.blothera.com"
        }
      ],
      "destination": "https://blothera.com/:1",
      "permanent": true
    },
    {
      "source": "http://blothera.com/:path*",
      "destination": "https://blothera.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.blothera.com/:path*",
      "destination": "https://blothera.com/:path*",
      "permanent": true
    }
  ],
  "trailingSlash": false
}
